a=[1,2,3,4,4,5,5,6,6,]
tmp=[]
for i in a:
     if len(tmp)>0:
          tmp2=i
          tmp3=tmp[len(tmp)-1]
          #print(tmp3)
          #print(tmp2)
          if int(tmp2) == int(tmp3)+1:
               tmp.append(tmp2)
          else:
               print""
     else:
          tmp.append(i)
print(tmp)
